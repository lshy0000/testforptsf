<resources>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="pickerviewwheeview2">
        <attr name="wheelviewv2_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr name="wheelview2_textSize" format="dimension"/>
        <attr name="wheelview2_textColorOut" format="color"/>
        <attr name="wheelview2_textColorCenter" format="color"/>
        <attr name="wheelview2_dividerColor" format="color"/>
        <attr name="wheelview2_dividerWidth" format="dimension"/>
        <attr name="wheelview2_lineSpacingMultiplier" format="float"/>
    </declare-styleable>
</resources>